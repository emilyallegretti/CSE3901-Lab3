<h1>Edit Course</h1>
<form action=<%=course_path(@course)%> method="patch">
    <label for="course_name">Course Name:</label>
    <input type="text" id="course_name" name="course_name" value=<%=@course.name%>><br>

    <label for="course_number">Course Number:</label>
    <input type="text" id="course_number" name="course_number" value=<%=@course.number%>><br>

    <label for="course_campus">Campus:</label>
    <input type="text" id="course_campus" name="course_campus" value=<%=@course.campus%>><br>

    <label for="course_term">Term:</label>
    <input type="text" id="course_term" name="course_term" value=<%=@course.term%>><br>

    <input type="submit" value="Submit">
<form>